{
  "name": "netlifytrading",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rimraf build",
    "dev": "cross-env NETLIFY_CLI_DISABLE_UPDATE_CHECK=1 netlify dev",
    "start": "cross-env NETLIFY_CLI_DISABLE_UPDATE_CHECK=1 netlify dev",
    "build": "npm run clean && shx mkdir -p build/netlify/functions && shx cp *.html build/ && shx cp *.css build/ && shx cp app.js ai-analyst.js ai-analyst-batch-table.js quant-screener.js _redirects build/ && shx cp -r professional build/professional && shx cp -r netlify/functions build/netlify/functions && shx cp -r data build/data",
    "serve": "npx http-server build -p 5000 -c-1",
    "generate:symbols": "node scripts/build-symbols.mjs",
    "peer-review:inventory": "node scripts/peer-review/generate-file-inventory.mjs",
    "qa:conflicts": "node scripts/qa/check-merge-conflicts.mjs",
    "test": "vitest run",
    "test:watch": "vitest",
    "scan:tiingo": "node test-token-detection.mjs",
    "deploy:staging": "node scripts/deploy-staging.mjs"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
    "csv-parse": "^5.5.6",
    "jsdom": "^27.0.0",
    "netlify-cli": "23.7.2",
    "node-fetch": "^3.3.2",
    "rimraf": "^6.0.1",
    "shx": "^0.4.0",
    "vitest": "^3.2.4",
    "xlsx": "^0.18.5"
  }
}
